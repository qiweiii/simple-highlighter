{
  "name": "Simple Highlighter",
  "version": "0.0.1",
  "manifest_version": 3,
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": [
      "node_modules/@webcomponents/custom-elements/custom-elements.min.js",
      "highlighter.js",
      "content.js",
      "storage.js"
    ]
  }],
  "permissions": ["storage"]
}